{
  "manifest_version": 3,
  "name": "ZAPatchex",
  "description": "pls work this time, onegaishimasu",
  "options_ui": {
    "page": "src/pages/options/index.html"
  },
  "action": {
    "default_popup": "src/pages/popup/index.html",
    "default_icon": {
      "32": "OWASP_ZAP_Logo.png"
    }
  },
  "icons": {
    "128": "OWASP_ZAP_Logo.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "http://localhost/*",
    "http://127.0.0.1/*",
    "http://zap/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "<all_urls>"
      ],
      "js": [
        "src/pages/content/index.tsx"
      ],
      "css": [
        "contentStyle.css"
      ]
    }
  ],
  "devtools_page": "src/pages/devtools/index.html",
  "web_accessible_resources": [
    {
      "resources": [
        "contentStyle.css",
        "OWASP_ZAP_Logo.png"
      ],
      "matches": []
    }
  ]
}